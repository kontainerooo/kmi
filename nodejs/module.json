{
    "name": "Node.js",
    "version": "0.0.2",
    "description": "Basic nodejs module",
    "type": 3,
    "provisionScript": "./provision.sh",
    "frontend": "./frontend.json",
    "env": {},
    "interfaces": {
        "HTTP": 3000
    },
    "cmd": {
        "start": "cd /var/node && npm start",
        "install": "cd /var/node && npm install",
        "stop": "pgrep node | kill $(xargs)",
        "status": "pgrep node | wc -l"
    },
    "resources": {
        "cpus": "2",
        "mem": 536870911,
        "swap": 1073741823
    }
}
