{
	"imports": ["status", "uploads", "log", "ssh"],
	"modules": [
	    {
	        "template": "status",
	        "parameters": {
	            "title": "Node App Status",
	            "start": "$KROO_CMD_START",
	            "stop": "$KROO_CMD_STOP",
	            "status": "$KROO_CMD_STATUS"
	        }
	    },
		{
			"template": "upload",
			"parameters": {
				"title": "Upload nodejs files",
				"dir": "/var/node",
				"list": "true"
			}
		},
		{
			"template": "log",
			"parameters": {
				"title": "NodeJS log",
				"logfile": "/var/log/node",
				"allowClear": "true"
			}
		},
		{
			"template": "ssh",
			"parameters": {
				"title": "SSH",
				"publicKey": "./ssh.pub",
				"logfile": "./ssh.log"
			}
		}
	]
}
